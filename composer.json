{
  "name": "drdplus/destruction",
  "license": "MIT",
  "type": "library",
  "description": "Drd+ destruction according to material, time and other aspects",
  "autoload": {
    "psr-4": {
      "DrdPlus\\Destruction\\": "Destruction"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DrdPlus\\Tests\\RollsOn\\": "vendor/drdplus/rolls-on/tests/RollsOn",
      "DrdPlus\\Tests\\Destruction\\": "tests/Destruction"
    }
  },
  "require": {
    "php": ">=7.3",
    "drdplus/armourer": "^2.2",
    "drdplus/rolls-on": "^2.0"
  },
  "require-dev": {
    "granam/exceptions-hierarchy": "^5.0",
    "granam/test-with-mockery": "^2.0",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~9.0",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "sort-packages": true
  }
}
